  <footer class="page-footer">
    <div class="col-md-4 d-flex align-items-center">
      <a href="/" class="mb-3 me-2 mb-md-0 text-body-secondary text-decoration-none lh-1">
        {{ site.title }}
      </a>
      <span class="mb-3 mb-md-0 text-body-secondary">&copy; 2024 {{ site.name }}</span>
    </div>

  {%- if site.footer.links %}
    <nav id="footer-nav" class="navbar navbar-expand-lg">
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarFooter" aria-controls="navbarFooter" aria-expanded="false" aria-label="Toggle navigation">
        <i class="navbar-toggler-icon"></i>
      </button>
      <div class="collapse navbar-collapse" id="navbarFooter">
        <ul class="navbar-nav">
        {% for item in site.footer.links -%}{% if item.url -%}
          <li class="nav-item">
            <a href="{{ item.url }}" {% if item.target %}target="{{ item.target }}" {% endif %}class="nav-link{% if item.class %} {{ item.class }}{% endif %}">
            {%- if item.icon %}
              <i class="{{ item.icon }}" aria-label="{{ item.label }}"></i>
            {%- endif %}
              <span class="ps-1{% if item.icon %} d-lg-none{% endif %}">{{ item.label | safe }}</span>
            </a>
          </li>
        {%- endif %}{%- endfor %}
        </ul>
      </div>
    </nav>
  {%- endif %}
  </footer>

